@misc{06AQ20040121_prof,
 author={Volker H {Strass}},
 title={{Physical oceanography during POLARSTERN cruise ANT-XXI/3 (EIFEX)}},
 year={2010},
 doi={10.1594/PANGAEA.733410},
 url={https://doi.org/10.1594/PANGAEA.733410},
 organization={Alfred Wegener Institute, Helmholtz Centre for Polar and Marine Research, Bremerhaven},
 note={Supplement to: Leach, Harry; Strass, Volker H (2019): Cyclonic eddies and upper thermocline fine-scale structures in the Antarctic Circumpolar Current. Ocean Dynamics, 69(2), 157-173, https://doi.org/10.1007/s10236-018-1241-x},
 abstract={The track of Polarstern Cruise ANTXXI/3-"EIFEX"- leaving from Cape Town on 21st January 2004 and arriving back in Cape Town on 25th March 2004. The purpose of this cruise was to conduct an iron fertilisation experiment in the ACC. The reason for using an eddy was that the water fertilised with iron sulphate would be trapped and relatively easy to follow. The first eddy (Eddy 1) selected on the basis of altimeter data was at about 50{\textdegree}S, 18{\textdegree}E. This eddy was surveyed during a period of 7 days between 25th January and 1st February 2004 by CTD/Rosette casts along five equally spaced meridional sections. Along the westernmost section, 17{\textdegree}E, the station spacing was 5 mi (9 km), and along the other 4 (17{\textdegree}40{\textquotesingle}, 18{\textdegree}20{\textquotesingle}, 19{\textdegree}00{\textquotesingle} and 19{\textdegree}40{\textquotesingle} E), it was 12 mi (22 km); the sections were completed systematically working from west to east. Investigation revealed that the initial chlorophyll concentration was too low for the fertilisation experiment and so this eddy was rejected, but not before a useful set of physical data had been obtained. Instead, a second eddy (Eddy 2), at about 49{\textdegree}S, 2{\textdegree}E was selected for the experiment and was ultimately occupied for a period of 40 days. Altogether, this eddy was investigated during the period 8th February to 20th March 2004; however, the data for the initial CTD/Rosette survey were collected during a period of 6 days between 14th and 20th February. The stations were evenly spaced 12 mi (22 km) apart meridionally and zonally, or 12{\textquotesingle} latitude and about 18.6{\textquotesingle} of longitude, with ten stations along each of eight equally spaced meridional sections between 1{\textdegree}19{\textquotesingle} and 3{\textdegree}29{\textquotesingle} E. The sections were collected systematically from west to east.},
 type={data set},
 urldate={2020-04-21},
 publisher={PANGAEA}
}

